import java.util.*;
class BinaryToString{
public static void main(String args[]){
  Scanner sc = new Scanner(System.in);
  Double d = sc.nextDouble();
  System.out.println(getBinaryString(d));
}
private static String getBinaryString(Double d){
  if(d<0||d>1){
   return "ERROR";
  }
  String s = "";
  int c=32;
  while(c!=0 && d!=0){
    d = d*2;
    if(d>=1){
      s=s+"1";
      d=d-1;
    } else{
      s=s+"0";
    }
    c--;
  }
  if(c==0 & d!=0){
    return "ERROR";
  }
  return s;
}
}
